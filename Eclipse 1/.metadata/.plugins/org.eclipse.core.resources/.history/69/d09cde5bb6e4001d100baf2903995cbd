package Training;
import java.util.*;
public class ex104{
    public static void main(String[] args) {
        ArrayList<Integer> a=new ArrayList<>();
        a.add(4);
        a.add(3);
        a.add(10);
        a.add(1);
        
        System.out.println(a);
        Collections.sort(a);
        System.out.println(a);
    }
}