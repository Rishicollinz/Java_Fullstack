<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->

	<bean id="emp1" class="autowire.Employee" autowire="constructor">
		<constructor-arg name="empId" value="2"></constructor-arg>
		<constructor-arg name="empName" value="Mouli"></constructor-arg>
		<constructor-arg name="empEmail" value="mouli@gmail.com"></constructor-arg>
	</bean>

	<bean id="pancard1" class="autowire.Pancard">
		<property name="panholderName" value="Rishi"></property>
		<property name="panNumber" value="867576"></property>
	</bean>

</beans>